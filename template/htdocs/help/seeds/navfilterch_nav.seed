	if (!isset($basefilter['this_dim'])&&(
		isset($basefilter['parent_dim_1'])
		||
		isset($basefilter['parent_dim_2'])
	)){
		#entity#_shownav($container, $cmd, 'This Dim Title','this_dim');	
